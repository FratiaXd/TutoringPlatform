@page "/coursepages/learning/coursecompleted/{CourseId:int}"

<div class="t-flex t-flex-col text-center t-mt-8">
    <h3 class="t-text-gray-600 t-text-2xl t-font-bold t-leading-none t-tracking-tight md:t-text-4xl">Course Completed</h3>
    <hr class="t-h-1 t-mx-4 md:t-mx-20 t-my-4 t-bg-gray-800 t-border-4 t-rounded md:t-my-2">
    <p class="t-mt-1 t-text-lg t-font-normal t-text-gray-500 dark:t-text-gray-400">Congratulations!</p>
</div>

@code {
    [Parameter]
    public int CourseId { get; set; }
}
